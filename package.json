{
  "name": "react-mobx-ts",
  "version": "1.0.0",
  "description": "Заготовка под проект на react стеке",
  "scripts": {
    "test": "jest",
    "start": "npm run clean && npm run addAssets && npm run develop",
    "build": "npm run test && npm run lint && npm run clean && npm run addAssets && npm run production && npm run clearScss",
    "develop": "webpack serve --config ./webpack/app/develop",
    "production": "webpack --config ./webpack/app/production",
    "addStyles": "mkdirp ./dist/assets && ncp ./styles-package ./dist/assets && npm run addComponentStyles",
    "addComponentStyles": "ncp ./node_modules/korus-ui/dist/styles ./dist/assets/css/korus-ui",
    "addAssets": "npm run addStyles && ncp ./public ./dist",
    "clean": "rimraf ./dist",
    "clearScss": "rimraf ./dist/assets/scss",
    "lint:ts": "tsc",
    "lint:js": "eslint src --color --ext .tsx,.ts --fix",
    "lint": "npm run stylelint && npm run format && npm run lint:ts && npm run lint:js",
    "stylelint": "stylelint --config .stylelintrc.js --fix ./src/**/*{.css,.scss}",
    "openMR": "git push -o merge_request.create -o merge_request.target=master -o merge_request.remove_source_branch origin feature/update-korus-ui",
    "format": "prettier --loglevel error --write ./src/**/*.{ts,tsx}"
  },
  "repository": {
    "type": "git",
    "url": "http://gitlab.id-mt.ru/ui/react-ts-start"
  },
  "keywords": [
    "webpack",
    "react",
    "mobx",
    "typescript",
    "korus-ui",
    "styles-esphere"
  ],
  "author": "Anton Golomazov<a.golomazov@id-mt.ru>",
  "license": "ISC",
